<Window x:Class="PracticFeb.Mainn"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PracticFeb"
        mc:Ignorable="d"
        Title="Mainn" Background="Aquamarine" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="55"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button FontWeight="Bold" Content="Корзина" Margin="676,10,10,10" FontSize="20" Click="Button_Click"/>
        <WrapPanel x:Name="wp_menu" Grid.Row="1" HorizontalAlignment="Center" Margin="0,80,0,0"/>
    </Grid>
</Window>
